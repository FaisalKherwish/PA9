// OBJECTS HPP main fruits objects not much changed
#include "ObjectManager.hpp"

class Watermelon : public ObjectManager
{
public:
	Watermelon();
	Watermelon(sf::Texture newSprite);

};

class Kiwi : public ObjectManager
{
public:
	Kiwi();
	Kiwi(sf::Texture newSprite);
};

class Orange : public ObjectManager
{
public:
	Orange();
	Orange(sf::Texture newSprite);
};

class Apple : public ObjectManager
{
public:
	Apple();
	Apple(sf::Texture newSprite);
};


class Coconut : public ObjectManager
{
public:
	Coconut();
	Coconut(sf::Texture newSprite);
};

class Dragonfruit : public ObjectManager
{
public:
	Dragonfruit();
	Dragonfruit(sf::Texture newSprite);
};

// OBJECTS CPP FILE also not much changed
#include "Objects.hpp"
#include "Utility.hpp"

Watermelon::Watermelon() : ObjectManager()
{
	this->getShape().setFillColor(sf::Color::Red);
	this->setPoints(1);
	this->setName("Watermelon");
	this->getShape().setPosition(sf::Vector2f(rng() % 880 + 100, 980));
}

Watermelon::Watermelon(sf::Texture newSprite) : ObjectManager()
{
	this->getShape().setFillColor(sf::Color::Red);
	this->setPoints(1);
	this->setName("Watermelon");
	this->setSprite(newSprite);
	this->getShape().setPosition(sf::Vector2f(rng() % 880 + 100, 980));
}
Kiwi::Kiwi()
{
	double z = rand();

	this->getShape().setFillColor(sf::Color::Red);
	this->setPoints(1);
	this->getShape().setPosition(sf::Vector2f(z, 1200));
	this->setName("Kiwi");
}

Kiwi::Kiwi(sf::Texture newSprite)
{
	double z = rand();

	this->getShape().setFillColor(sf::Color::Red);
	this->setPoints(1);
	this->getShape().setPosition(sf::Vector2f(z, 1200));
	this->setName("Kiwi");
}

